{
  "publisher": "slipwayhq",
  "name": "jsx",
  "description": "Takes an jsx string and returns a canvas.",
  "version": "0.0.1",
  "input": {
    "properties": {
      "width": { "type": "uint32" },
      "height": { "type": "uint32" },
      "jsx": { "type": "string"}
    },
    "optionalProperties": {
      "utils_js": { "type": "string" },
      "debug": { "type": "boolean" },
      "fonts": { "elements": { "type": "string" }},
      "data": {},
      "embed_font": { "type": "boolean" },
      "background_color": { "type": "string"}
    }
  },
  "output": "canvas",
  "rigging": {
    "jsx__transpile": {
      "component": "slipwayhq.jsx__transpile.0.5.4",
      "input": {
        "jsx": "$$.input.jsx"
      }
    },
    "jsx__svg": {
      "component": "slipwayhq.jsx__svg.0.5.4",
      "allow": [
        { "permission": "fonts" },
        { "permission": "http" }
      ],
      "input": {
        "width": "$$.input.width",
        "height": "$$.input.height",
        "jsx_js": "$$.jsx__transpile.js",
        "utils_js": "$$?input.utils_js",
        "debug": "$$?input.debug",
        "fonts": "$$?input.fonts",
        "data": "$$?input.data",
        "embed_font": "$$?input.embed_font"
      }
    },
    "output": {
      "component": "slipwayhq.svg.0.6.1",
      "allow": [
        { "permission": "fonts" },
        { "permission": "http" }
      ],
      "input": {
        "width": "$$.input.width",
        "height": "$$.input.height",
        "svg": "$$.jsx__svg.svg",
        "background_color": "$$?input.background_color"
      }
    }
  }
}