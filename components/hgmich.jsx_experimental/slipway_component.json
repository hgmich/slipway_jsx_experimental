{
  "publisher": "hgmich",
  "name": "jsx_experimental",
  "description": "Takes an jsx string and returns a canvas.",
  "version": "0.0.1",
  "input": {
    "properties": {
      "width": { "type": "uint32" },
      "height": { "type": "uint32" },
      "jsx": { "type": "string" }
    },
    "optionalProperties": {
      "utils_js": { "type": "string" },
      "debug": { "type": "boolean" },
      "fonts": { "elements": { "type": "string" } },
      "data": {},
      "embed_font": { "type": "boolean" },
      "background_color": { "type": "string" },
      "disable_smoothing": {
        "type": "boolean",
        "metadata": {
          "description": "Whether or not to disable antialiasing of shape edges (helps with display on E-Paper displays)"
        }
      }
    }
  },
  "output": "canvas",
  "rigging": {
    "jsx__transpile": {
      "component": "hgmich.jsx_experimental__transpile.0.6.0-pre1",
      "input": {
        "jsx": "$$.input.jsx"
      }
    },
    "jsx__svg": {
      "component": "hgmich.jsx_experimental__svg.0.6.0-pre1",
      "allow": [{ "permission": "fonts" }, { "permission": "http" }],
      "input": {
        "width": "$$.input.width",
        "height": "$$.input.height",
        "jsx_js": "$$.jsx__transpile.js",
        "utils_js": "$$?input.utils_js",
        "debug": "$$?input.debug",
        "fonts": "$$?input.fonts",
        "data": "$$?input.data",
        "embed_font": "$$?input.embed_font"
      }
    },
    "output": {
      "component": "hgmich.svg_experimental.0.7.0-pre1",
      "allow": [{ "permission": "fonts" }, { "permission": "http" }],
      "input": {
        "width": "$$.input.width",
        "height": "$$.input.height",
        "svg": "$$.jsx__svg.svg",
        "background_color": "$$?input.background_color",
        "disable_smoothing": "$$?input.disable_smoothing"
      }
    }
  }
}
